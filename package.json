{
  "name": "tree-sitter-scribl",
  "version": "0.1.0",
  "description": "Scribl grammar for tree-sitter",
  "repository": "https://github.com/scribhneoir/tree-sitter-scribl",
  "license": "MIT",
  "author": {
    "name": "Josiah McCracken",
    "email": "josiah@scribhneoir.com",
    "url": "https://www.scribhneoir.com/"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "tree_sitter_scribl"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.22.4"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "prepare": "tree-sitter generate",
    "install": "tree-sitter generate && node-gyp-build",
    "postinstall": "npm run build:bindings",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "pretest": "tree-sitter generate",
    "test": "tree-sitter test",
    "clean": "rm -rf build/ src/grammar.json src/node-types.json src/parser.c src/tree_sitter/",
    "generate": "tree-sitter generate",
    "build:bindings": "node-gyp-build",
    "build:wasm": "tree-sitter build --wasm",
    "build": "npm run clean && npm run generate && npm run build:bindings",
    "rebuild": "npm run build"
  },
  "packageManager": "pnpm@10.24.0"
}
