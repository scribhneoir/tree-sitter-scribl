{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "scribl",
      "camelcase": "Scribl",
      "title": "Scribl",
      "scope": "source.scribl",
      "file-types": ["scribl"],
      "injection-regex": "^scribl$",
      "class-name": "TreeSitterScribl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Scribl grammar for tree-sitter",
    "authors": [
      {
        "name": "Josiah McCracken",
        "email": "josiah@scribhneoir.com",
        "url": "https://www.scribhneoir.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/scribhneoir/tree-sitter-scribl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
